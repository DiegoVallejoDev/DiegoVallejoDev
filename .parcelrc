{
  "extends": ["@parcel/config-default"],
  "reporters": ["...", "parcel-reporter-static-files-copy"],
  "transformers": {
    "*.{jpg,jpeg,png,gif,webp,avif}": ["@parcel/transformer-image"]
  },
  "optimizers": {
    "*.{jpg,jpeg,png,gif}": ["@parcel/optimizer-image"]
  },
  "compressors": {
    "*.{html,css,js,svg,map}": [
      "...",
      "@parcel/compressor-gzip",
      "@parcel/compressor-brotli"
    ]
  }
}
